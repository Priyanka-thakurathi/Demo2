import java.util.*;

class detail
{
public static void main(String[] args)
{
int roll_no;
String name;
Scanner in=new Scanner(System.in);
System.out.println("Enter your roll number :");
roll_no=in.nextInt();
System.out.println("Enter your name :");
name=in.nextLine();
System.out.println("%s"+name);
System.out.println("%d"+roll_no);
}
}
